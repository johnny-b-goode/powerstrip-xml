<?xml version="1.0" encoding="UTF-8"?>
<plugin>
	<name>XMLPlugin</name>
	<class>net.scientifichooliganism.xmlplugin.XMLPlugin</class>
	<configuration>
		<module>XMLPlugin</module>
		<sequence>0</sequence>
		<key>active</key>
		<value>true</value>
	</configuration>
	<configuration>
		<module>XMLPlugin</module>
		<sequence>1</sequence>
		<key>storage</key>
		<value>false</value>
	</configuration>
	<action>
		<name>nodeFromObject</name>
		<description>Create an XMLNode from an Object</description>
		<module>XMLPlugin</module>
		<class>net.scientifichooliganism.xmlplugin.XMLPlugin</class>
		<!--url></url-->
		<method>nodeFromObject</method>
	</action>
	<action>
		<name>objectFromNode</name>
		<description>Create an object from an XMLNode</description>
		<module>XMLPlugin</module>
		<class>net.scientifichooliganism.xmlplugin.XMLPlugin</class>
		<!--url></url-->
		<method>objectFromNode</method>
	</action>
	<action>
		<name>objectFromString</name>
		<description>Create an object from an XML String</description>
		<module>XMLPlugin</module>
		<class>net.scientifichooliganism.xmlplugin.XMLPlugin</class>
		<!--url></url-->
		<method>objectFromString</method>
	</action>
	<action>
		<name>stringFromObject</name>
		<description>Create a XML string from an object</description>
		<module>XMLPlugin</module>
		<class>net.scientifichooliganism.xmlplugin.XMLPlugin</class>
		<!--url></url-->
		<method>stringFromObject</method>
	</action>
</plugin>